<h1>ログイン</h1>
<div class="container">
  <%= form_with(scope: :session, url: sessions_path, local: true) do |f| %>

    <%= f.label :email %>
    <%= f.email_field :email %>

    <%= f.label :password %>
    <%= f.password_field :password %>
    <p><br>
      <%= f.submit "ログイン" %>
    </p>
  <% end %>
</div>
